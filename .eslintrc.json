{
    "env": {
        "node": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "globals": {
        "Map": true,
        "Promise": true,
        "Set": true,
        "describe": true,
        "it": true,
        "afterEach": true,
        "expect": true,
        "browser": true
    },
    "rules": {
        "no-extra-parens": 2,
        "no-prototype-builtins": 2,
        "no-template-curly-in-string": 2,
        "semi": [2, "always"]
    }
}
